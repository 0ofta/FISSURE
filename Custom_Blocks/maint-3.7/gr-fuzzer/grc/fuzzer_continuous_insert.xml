<?xml version="1.0"?>
<block>
  <name>continuous_insert</name>
  <key>fuzzer_continuous_insert</key>
  <category>fuzzer</category>
  <import>import fuzzer</import>
  <make>fuzzer.continuous_insert($vector)</make>

  <param>
      <name>Starting Vector</name>
      <key>vector</key>
      <value>0, 0, 0</value>
      <type>int_vector</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <sink>
    <name>packet_in</name>
    <type>message</type>
  </sink>  


  <source>
    <name>out</name>
    <type>byte</type>
  </source>
  
</block>
